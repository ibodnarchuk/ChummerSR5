<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="8dp"
    android:paddingLeft="8dp"
    android:paddingRight="8dp"
    android:paddingTop="8dp">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Name"
        android:id="@+id/fragment_technomancer_complex_form_display_name_textview"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Target"
        android:id="@+id/fragment_technomancer_complex_form_display_target_label_textview"
        android:layout_below="@+id/fragment_technomancer_complex_form_display_name_textview"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Target Text"
        android:id="@+id/fragment_technomancer_complex_form_display_target_textview"
        android:layout_below="@+id/fragment_technomancer_complex_form_display_target_label_textview"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Duration"
        android:id="@+id/fragment_technomancer_complex_form_display_duration_label_textview"
        android:layout_alignTop="@+id/fragment_technomancer_complex_form_display_target_label_textview"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Duration Text"
        android:id="@+id/fragment_technomancer_complex_form_display_duration_textview"
        android:layout_below="@+id/fragment_technomancer_complex_form_display_duration_label_textview"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Fading Value"
        android:id="@+id/fragment_technomancer_complex_form_display_fading_label_textview"
        android:layout_alignTop="@+id/fragment_technomancer_complex_form_display_duration_label_textview"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/fragment_technomancer_complex_form_display_fading_textview"
        android:layout_below="@+id/fragment_technomancer_complex_form_display_fading_label_textview"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Summary"
        android:id="@+id/fragment_technomancer_complex_form_display_summary_textView"
        android:paddingTop="20dp"
        android:layout_below="@+id/fragment_technomancer_complex_form_display_target_textview"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Page"
        android:id="@+id/fragment_technomancer_complex_form_display_page_textView"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Book"
        android:id="@+id/fragment_technomancer_complex_form_display_book_textView"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:layout_toLeftOf="@+id/fragment_technomancer_complex_form_display_page_textView"
        android:layout_toStartOf="@+id/fragment_technomancer_complex_form_display_page_textView" />

</RelativeLayout>